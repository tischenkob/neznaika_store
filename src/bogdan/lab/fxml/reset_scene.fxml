<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" style="-fx-background-color: #36393F; -fx-opacity: 100;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="bogdan.lab.controllers.ResetController">
   <children>
      <AnchorPane layoutY="-14.0" prefHeight="600.0" prefWidth="1000.0" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="-14.0">
         <children>
            <Button fx:id="backButton" layoutX="32.0" layoutY="40.0" mnemonicParsing="false" onAction="#backToPassword" style="-fx-padding: 0; -fx-background-color: transparent;" text="&lt;Back" textFill="#5e7bd3">
               <font>
                  <Font name="Calibri Light" size="27.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <PasswordField fx:id="passwordTXT" layoutX="277.0" layoutY="292.0" onAction="#changePassword" prefHeight="44.0" prefWidth="347.0" style="-fx-background-color: #E8F0FE;" />
            <Text fill="#ebebeb" layoutX="277.0" layoutY="287.0" strokeType="OUTSIDE" strokeWidth="0.0" text="New Password">
               <font>
                  <Font name="Calibri Light" size="18.0" />
               </font>
            </Text>
            <Button id="login_button" fx:id="changeButton" layoutX="306.0" layoutY="355.0" mnemonicParsing="false" onAction="#changePassword" prefHeight="18.0" prefWidth="289.0" style="-fx-background-color: #7289DA; -fx-text-fill: #FFFFFF;" text="Change password" textAlignment="CENTER" textFill="#039be5" textOverrun="WORD_ELLIPSIS" wrapText="true">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <font>
                  <Font name="Calibri Light" size="24.0" />
               </font>
            </Button>
            <Text fx:id="wrongText" fill="#d34848" layoutX="573.0" layoutY="215.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Wrong" visible="false">
               <font>
                  <Font name="Calibri Light" size="18.0" />
               </font>
            </Text>
            <PasswordField fx:id="tokenTXT" layoutX="277.0" layoutY="220.0" prefHeight="44.0" prefWidth="347.0" style="-fx-background-color: #E8F0FE;" />
            <Text fill="#ebebeb" layoutX="277.0" layoutY="215.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Token">
               <font>
                  <Font name="Calibri Light" size="18.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
